
@{
    ViewBag.Title = "uploadCSV";
}

<h2>Cargar CSV</h2>

@using (Html.BeginForm("uploadCSV", "Proveedor", FormMethod.Post, new { enctype = "multipart/form-dta" }))
{
    <div class=" form-group">
        <label class="control-label col-md-2">Cargar CSV</label>
        <div class="col-md-10">
            <input type="file" name="fileForm" class="form-control" />

        </div>

    </div>


    <div class=" form-group">
        <div class="col-md-10">
            <input type="submit" value="Enviar"/>

        </div>

    </div>
}

